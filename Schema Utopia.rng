   <define name="tei_utopiasurface">
      <element name="surface">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">defines a written surface as a two-dimensional coordinate space, optionally grouping one or more graphic representations of that space, zones of interest within that space, and transcriptions of the writing within them. [11.1. Digital Facsimiles 11.2.2. Embedded Transcription]</a:documentation>
         <group>
      
            <zeroOrMore>
               <choice>
                  <ref name="tei_utopiamodel.global"/>
                  <ref name="tei_utopiamodel.labelLike"/>
                  <ref name="tei_utopiamodel.graphicLike"/>
               </choice>
            </zeroOrMore>
      
            <zeroOrMore>
               <group>
        
                  <choice>
            
            
            
                     <ref name="tei_utopiasurface"/>
                     <ref name="tei_utopiasurfaceGrp"/>
                  </choice>
        
        
                  <zeroOrMore>
                     <ref name="tei_utopiamodel.global"/>
                  </zeroOrMore>
        
               </group>
            </zeroOrMore>
         </group>
         <ref name="tei_utopiaatt.global.attributes"/>
         <ref name="tei_utopiaatt.coordinated.attributes"/>
         <ref name="tei_utopiaatt.declaring.attributes"/>
         <ref name="tei_utopiaatt.typed.attributes"/>
